<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="co.trexcode.treestore">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher_2"
            android:label="Trees KKU Application"
            android:roundIcon="@mipmap/ic_launcher_2"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".GoodDetailActivity">
        </activity>
        <activity android:name=".BasketDetailActivity">
        </activity>
        <activity android:name=".AuthActivity">
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".HistoryActivity">
        </activity>
        <activity android:name=".HistoryDetailActivity">
        </activity>
        <activity android:name=".AdminDashBoardActivity">
        </activity>
        <activity android:name=".AdminGoodsList">
        </activity>
        <activity android:name=".AdminGoodDetailActivity">
        </activity>
        <activity android:name=".AdminGoodsAddActivity">
        </activity>
        <activity android:name=".AdminOrderActivity">
        </activity>
        <activity android:name=".AdminOrderDetailActivity">
        </activity>
    </application>

</manifest>